<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>REPORTE AL DIA</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresion.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NUMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>REPORTE AL DIA</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>	
		
						<ROW>
										<COL ALIGN="RIGHT" TYPE="EXPRESSION"  CELLCLASS="TOTALES">"TOTALES"</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("ventas_dia")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("ventas_canceladas")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("ventas_mes")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("cobranza_vencida")</COL>
										
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("cobranza_no_vencida")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("cuenta_bancaria_hoy")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("cuenta_bancaria_mes")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("ingreso_caja_chica_hoy")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("ingreso_caja_chica_mes")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">""</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("egreso_caja_chica_hoy")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("egreso_caja_chica_mes")</COL>
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">""</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("saldo_caja_chica")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("pagos_cxp")</COL>
										
										<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("pagos_cxp_his")</COL>


							
							
						</ROW>		
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="groupReportes" EXPRESSION="reportes" >
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"SUCURSAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"VENTAS DEL DIA"</COL>
			    <COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"VENTAS CANCELADAS DEL DIA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"VENTAS MENSUALES"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"COBRANZA VENCIDA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"COBRANZA NO VENCIDA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"INGRESOS A CUENTAS BANCARIAS DEL DIA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"INGRESOS A CUENTAS BANCARIAS MENSUAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"INGRESOS A CAJA CHICA DEL DIA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"INGRESOS A CAJA CHICA MENSUAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"INGRESOS A CAJA CHICA GLOBAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"EGRESOS A CAJA CHICA DEL DIA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"EGRESOS A CAJA CHICA MENSUAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"EGRESOS A CAJA CHICA GLOBAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"SALDO CAJA CHICA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"PAGOS DE CUENTAS POR PAGAR DEL MES"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"PAGOS DE CUENTAS POR PAGAR GLOBAL"</COL>
				</ROW>	
				</HEADER>
				
				<FOOTER>			
				
					
					
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="17" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
					
					
					
					
					
				</FOOTER>
				
				<GROUP NAME="sucursal" EXPRESSION="sucursal">
					<HEADER>		
					</HEADER>				
					
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >sucursal</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">ventas_dia</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2" >ventas_canceladas</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2" >ventas_mes</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2" >cobranza_vencida</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">cobranza_no_vencida</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">cuenta_bancaria_hoy</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">cuenta_bancaria_mes</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">ingreso_caja_chica_hoy</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">ingreso_caja_chica_mes</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">ingreso_caja_chica_global</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">egreso_caja_chica_hoy</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">egreso_caja_chica_mes</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">egreso_caja_chica_global</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">saldo_caja_chica</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">pagos_cxp</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2" >pagos_cxp_his</COL>
					</ROW>
				</FIELDS>
				</GROUP>								
		
		
						<FOOTER>			
				
					
					
				
				
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="17" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
					
					
					
					
					
				</FOOTER>
		
		</GROUP>
		
	</GROUPS>
</REPORT>