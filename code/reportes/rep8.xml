<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Corte de Caja :: Reporte Detallado</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="8" CELLCLASS="CELDAGRANTOTAL">Saldo al Momento</COL>
				<COL ALIGN="LEFT"  NUMBERFORMATEX="2" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" TYPE="EXPRESSION">$this->getSum("Monto No Confirmado")</COL>
				<COL ALIGN="LEFT"  NUMBERFORMATEX="2" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" TYPE="EXPRESSION">$this->getSum("monto_confirmado")</COL>
			</ROW>
			<ROW>			
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Reporte Detallado :: Corte de Caja </U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">"Filtro utilizado: ".$this->getParameter("filtro")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; 
						background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" 
						class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>			
			<ROW>	
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="90" >"Sucursal"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="30" >"Fecha"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Tipo Movimiento"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="140" >"Forma"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"Terminal"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Documento"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Confirmado"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Documento Relacionado"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Monto Confirmado"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="80" >"Monto No Confirmado"</COL>
				</ROW>	
		</HEADER>
	</PAGE>
	<GROUPS>
    	<GROUP NAME="grupoSucursal" EXPRESSION="id_sucursal">
			<FOOTER>
				<ROW>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL> 
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" >Total Sucursal</COL>
				   <COL ALIGN="LEFT" CELLCLASs="TOTALPARCIAL"   TEXTCLASS="BOLD" NUMBERFORMATEX="2" TYPE="EXPRESSION">$this->getSum("monto_confirmado")</COL>
				   <COL ALIGN="RIGHT" CELLCLASS="TOTALPARCIAL" ></COL>				  
				</ROW>
         	</FOOTER>
        	<FIELDS>
				<ROW>					
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Sucursal</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Fecha</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Tipo Movimiento</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Forma</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Terminal</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Documento</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Confirmado</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Doc Rel</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >monto_confirmado</COL>
					<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >Monto No Confirmado</COL>
					</ROW>
        	</FIELDS>
    	</GROUP>
 	</GROUPS>
</REPORT>