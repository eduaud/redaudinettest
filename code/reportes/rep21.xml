<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Reporte de Egresos</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>SYS AND WEB</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Egresos (Resumido)</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">"Filtro utilizado: " . insert_where()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="groupSuc">
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ID"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"BANCO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FORMA DE PAGO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"A NOMBRE DE"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TOTAL CC"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"MONTO PAGADO CXP"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TOTAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"CUENTAS POR PAGAR"</COL>
				</ROW>	
				</HEADER>
				
				<FOOTER>			
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="16" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
				</FOOTER>				
				
				<GROUP NAME="depositos" >
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_egreso</COL>					
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha_egreso</COL>	
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >banco</COL>		
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >forma_pago</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >a_nombre</COL>		
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >monto_contables</COL>	
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >monto_pagado_cxp</COL>		
						<COL TYPE="FIELD" ALIGN="RIGHT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >monto_egreso</COL>		
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_egreso</COL>	
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >cxps</COL>
					</ROW>
				</FIELDS>
				<FOOTER>
						<ROW>
							<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
						</ROW>	
						<ROW>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" COLSPAN="5" CELLCLASS="GRANTOTAL">"GRAN TOTAL"</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_contables")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_pagados")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_egresos")</COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
						 </ROW>
			  </FOOTER>
				</GROUP>		
				
		</GROUP>
	</GROUPS>
</REPORT>