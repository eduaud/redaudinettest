<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Pedidos Resumido</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Pedidos Resumido</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>	
			<ROW>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" COLSPAN="7" CELLCLASS="GRANTOTAL">"GRAN TOTAL"</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_fletes")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_productos")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_total")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_pagos")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("suma_saldo")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="groupProducto" EXPRESSION="sucursal" >
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"SUCURSAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"PEDIDO"</COL>
			    <COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="250" >"CLIENTE"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="250" >"VENDEDOR"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="70" >"FECHA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"ESTATUS DE PAGO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"ESTATUS DE PEDIDO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"FLETES"</COL>	
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"PRODUCTOS"</COL>	
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"IMPORTE"</COL>	
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"MONTO PAGADO"</COL>	
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="100" >"SALDO"</COL>	
				</ROW>	
				</HEADER>
				<GROUP NAME="SUCURSALES" EXPRESSION="sucursal">
					<HEADER>		
					</HEADER>				
					
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >sucursal</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >pedido</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >cliente</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >vendedor</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fec_alta</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pago</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pedido</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fletes</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >productos</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >importe</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >pagado</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >saldo</COL>
					</ROW>
				</FIELDS>
				</GROUP>
				<FOOTER>			
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="16" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
				</FOOTER>				
		</GROUP>
	</GROUPS>
</REPORT>