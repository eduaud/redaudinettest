<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="Facturas">
	<TITLE>Pedidos Resumido</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Facturas</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>	
			<ROW>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" COLSPAN="9" CELLCLASS="GRANTOTAL">"GRAN TOTAL"</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("subtotal")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("iva")</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("total")</COL>
		
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="groupProducto" EXPRESSION="sucursal" >
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"SUCURSAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"FACTURA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"FOLIO CFDI"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"PEDIDO"</COL>
			    <COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="250" >"RAZON SOCIAL"</COL>
		    <COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="250" >"	CLIENTE"</COL>

				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="250" >"RFC"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="70" >"FECHA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="70" >"ESTATUS"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"SUBTOTAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"IVA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"TOTAL"</COL>



				</ROW>	
				</HEADER>
				<GROUP NAME="SUCURSALES" EXPRESSION="sucursal">
					<HEADER>		
					</HEADER>				
					
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >nombre_sucursal</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_factura</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >folio_cfd</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_pedido</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >nombre_razon_social</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >nombre_cliente</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >rfc</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >nombre_cancelada</COL>
						
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2" >subtotal</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">iva</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">total</COL>
					</ROW>
				</FIELDS>
				</GROUP>
				<FOOTER>			
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="16" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
				</FOOTER>				
		</GROUP>
	</GROUPS>
</REPORT>