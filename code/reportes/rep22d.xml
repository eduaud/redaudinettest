<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Reporte de Depositos Bancarios</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER1</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Depositos Bancarios (Detallado)</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("filtro")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
			<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ID"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"BANCO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"CUENTA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TOTAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS"</COL>
				</ROW>	
		</HEADER>
		
		<FOOTER>			
			<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>	
			<ROW>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" COLSPAN="3" CELLCLASS="GRANTOTAL">"GRAN TOTAL"</COL>
					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("total")</COL>

					<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("monto_ingresado")</COL>
					<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
					<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
			</ROW>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getValue("total_registros")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="deposito_bancario" EXPRESSION="deposito_bancario">
				<HEADER>
					<ROW>
						<COL TYPE="EXPRESSION" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("deposito_bancario")</COL>
						<COL TYPE="EXPRESSION" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("fecha_dmuestra")</COL>
						<COL TYPE="EXPRESSION" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("banco")</COL>
						<COL TYPE="EXPRESSION" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("cuenta_banco")</COL>
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400" NUMBERFORMATEX="2">$this->getValue("total_deposito")</COL>
						<COL TYPE="EXPRESSION" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("estatus_deposito")</COL>
					</ROW>
			</HEADER>
				<GROUP NAME="detalles" >
						<HEADER>
						<ROW>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"PEDIDO"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"FECHA"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"CLIENTE"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"TOTAL"</COL>


								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"MONTO DEPOSITADO"</COL>	
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS DEL PEDIDO"</COL>		
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS PAGO"</COL>										
						</ROW>
				</HEADER>
				<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >pedido</COL>				
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha_detallem</COL>	
						<COL TYPE="FIELD" ALIGN="CENTER" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >cliente</COL>	
						<COL TYPE="FIELD" ALIGN="RIGHT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">total</COL>


						<COL TYPE="FIELD" ALIGN="RIGHT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">monto_ingresado</COL>	
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pedido</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" VALIGN="TOP" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pago</COL>						
					</ROW>
				</FIELDS>
				<FOOTER>
						<ROW>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" COLSPAN="3" CELLCLASS="TOTALES">"TOTALES"</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("total")</COL>
							
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("monto_ingresado")</COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
						</ROW>
						<ROW>
							<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
						</ROW>
				</FOOTER>
				</GROUP>		
				
		</GROUP>
	</GROUPS>
</REPORT>