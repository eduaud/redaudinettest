<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>REPORTE DE PROVEEDORES</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresiÃ³n.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NUMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>PROVEEDORES</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>		
		<GROUP NAME="groupReportes" EXPRESSION="reportes" >
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ID"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TIPO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"RAZON SOCIAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"RFC"</COL>
			    <COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TELEFONO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"CORREO"</COL>
				</ROW>	
				</HEADER>
				
				<FOOTER>			
					<ROW>	
						<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" COLSPAN="16" CELLCLASSODD="ODD" >" "</COL>
					</ROW>
				</FOOTER>
				
				<GROUP NAME="sucursal" EXPRESSION="sucursal">
					<HEADER>		
					</HEADER>				
					
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_proveedor</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >tipo</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >razon_social</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >rfc</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >telefono</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >correo</COL>
					</ROW>
				</FIELDS>
				</GROUP>								
		</GROUP>
	</GROUPS>
</REPORT>