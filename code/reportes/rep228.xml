<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="Facturas">
	<TITLE>Movimientos de Salida</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresión.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="14" WIDTH="750">" "</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="14" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#052755; color:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/audicel.png"/><BR/>
									<b>AUDINET</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>MOVIMIENTOS DE SALIDA</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="14" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="14" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="14" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="14" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="14" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="14" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="8" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">"Total General"</COL>
				<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL" NUMBERFORMATEX="2">$this->getSum("importe")</COL>
			</ROW>
			<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="14" HEIGHT="10" WIDTH="750">" "</COL>
			</ROW>	
			<ROW>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="14" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>
		</FOOTER>
	</PAGE>
	<GROUPS>
		<GROUP NAME="General" EXPRESSION="id_detalle" >
			<HEADER>
				<ROW>					
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"ID MOVIMIENTO"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"SUBTIPO MOVIMIENTO "</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"ALMACEN"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"FECHA"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"HORA"</COL>					
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"ID DOC"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"PROVEEDOR"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"CLIENTE"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"TÉCNICO"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"USUARIO"</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"OBSERVACIONES"</COL>
				</ROW>
			</HEADER>
			<FOOTER>
				<ROW>
					<COL ALIGN="CENTER" COLSPAN="2" TYPE="EXPRESSION">" "</COL>
					<COL ALIGN="RIGHT" COLSPAN="6" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">"Total "</COL>
					<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL" NUMBERFORMATEX="2">$this->getSum("importe")</COL>
				</ROW>
			</FOOTER>
				<GROUP NAME="detalle" EXPRESSION="id_detalle" >
					<HEADER>
						<ROW>					
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >id_movimiento</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >subtipo_movimiento</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >nombre_almacen</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >fecha_movimiento</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >hora_movimiento</COL>					
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >idoc</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >proveedor</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >Cliente</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >Tecnico</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >nombre_usuario</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASS="EVEN" TEXTCLASS="BOLD" >observaciones_movimiento</COL>
						</ROW>
						<ROW>
							<COL ALIGN="CENTER" COLSPAN="2" TYPE="EXPRESSION">" "</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"CLAVE"</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"NOMBRE DEL PRODUCTO"</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"ID FACTURA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"LOTE"</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"CANTIDAD"</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"COSTO"</COL>					
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"IMPORTE"</COL>
							<!--<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" >"BITACORA RUTA"</COL>-->
						</ROW>
					</HEADER>
					
					<FIELDS>
						<ROW>
							<COL ALIGN="CENTER" COLSPAN="2" TYPE="EXPRESSION">" "</COL>
							<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >clave</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >nombre_producto</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_factura</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >lote</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >cantidad</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >costo</COL>						
							<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2">importe</COL>
							<!--<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_bitacora_ruta</COL>-->						
							
						</ROW>
					</FIELDS>
				</GROUP>
		</GROUP>
	</GROUPS>
</REPORT>