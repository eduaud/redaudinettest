<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Ventas por Sucursal</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresion.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NUMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Ventas por Sucursal</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
				
			<FOOTER>			
					
						<ROW>
							<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
						</ROW>	
						<ROW>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" COLSPAN="3" CELLCLASS="GRANTOTAL">"GRAN TOTAL"</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("total_suma")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("pagos_suma")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL">$this->getSum("saldo_suma")</COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" CELLCLASS="GRANTOTAL"></COL>
						</ROW>
			  
				</FOOTER>	
		
	</PAGE>

	<GROUPS>		
		<GROUP NAME="Ventas" EXPRESSION="no" >
				<HEADER>				 	
				<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400">"SUCURSAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400">"FECHA"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"PEDIDO"</COL>
			    <COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"IMPORTE"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"MONTO PAGADO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"SALDO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS DE PAGO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS DE PEDIDO"</COL>
				</ROW>	
				</HEADER>
				<GROUP NAME="no" EXPRESSION="no">
					<HEADER>		
					</HEADER>				
					
					<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >sucursal</COL>
						<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha_pedido</COL>
						<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >pedido</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >total_muestra</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >pagos_muestra</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >saldo_muestra</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pago</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_pedido</COL>
						
						
					</ROW>
				</FIELDS>
				</GROUP>
								
		</GROUP>
	</GROUPS>
</REPORT>