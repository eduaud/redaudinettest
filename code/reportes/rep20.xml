<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Cuentas por Pagar</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresion.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
		
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>ERP SYSANDWEB</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>CUENTAS POR PAGAR</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">"Filtro utilizado: ".$this->getParameter("filtro")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		
		<ROW>					
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"CXP"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"DOC. EMITIDO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"PROVEEDOR"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"REEMBOLSAR A"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FOLIO DOCUMENTO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FOLIO FISCAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA DOCUMENTO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA VENCIMIENTO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"TOTAL"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA ULTIMO PAGO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"MONTO ULTIMO PAGO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"SALDO"</COL>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS"</COL>
		</ROW>	
				
		</HEADER>
		<FOOTER>			
			<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  COLSPAN="8">"GRAN TOTAL" </COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("total_cxp")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("montoUltPago")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("saldo_cxp")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="GRANTOTAL" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
			</ROW>
			
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NÚMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
								
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Página ".$this->getPageNum()</COL>
			</ROW>		
			
		</FOOTER>
		
	</PAGE>

	<GROUPS>		
		<GROUP NAME="empleado" EXPRESSION="empleado" >
				<FIELDS>
					<ROW>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >id_cxp</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >tipo_documento_emitido</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >proveedor</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >empleado</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >numero_documento</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >folio_fiscal</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha_documento_muestra</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >fecha_vencimiento</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">total_cxp</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">fechaPago</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">montoUltPago</COL>
						<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD"  NUMBERFORMATEX="2">saldo_cxp</COL>
						<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >estatus_cxp</COL>
						
						
						
					</ROW>
				</FIELDS>
		<FOOTER>			
			
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="TOTALES" TEXTCLASS="BOLD"  COLSPAN="7">$this->getValue("info_total")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" >TOTAL</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("total_cxp")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("montoUltPago")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" NUMBERFORMATEX="2">$this->getSum("saldo_cxp")</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
				<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASS="MONTOSTOTALES" TEXTCLASS="BOLD"  WIDTH="400" >""</COL>
			</ROW>
				
			
		</FOOTER>
		</GROUP>
		
	</GROUPS>

</REPORT>