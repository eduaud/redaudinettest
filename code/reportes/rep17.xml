<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Ordenes de Compra</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresiÃ³n.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NUMERO TOTAL DE REGISTROS: " . $this->getValue("contador")</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Ordenes de Compra</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("filtro")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>
		<GROUP NAME="id" EXPRESSION="id">
			<HEADER>
					<ROW>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ORDEN DE COMPRA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"PROVEEDOR"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"FECHA DE ENTREGA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"MERCANCIA PUESTA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="400" >"ESTATUS"</COL>
							
					</ROW>
					
					<ROW>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("id")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("proveedor")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("fecha")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("fecha_entrega")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("mercancia_puesta")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">$this->getValue("estatus")</COL>
					</ROW>
			</HEADER>
					
			<GROUP NAME="detalle">
				<HEADER>
						<ROW>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"PRODUCTO"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"CANTIDAD"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"PRECIO"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="400" >"IMPORTE"</COL>
						</ROW>
				</HEADER>
				<FIELDS>
					<ROW>
							
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">producto</COL>
							<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">cantidad</COL>
							<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">precio</COL>
							<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="400">importe</COL>
					</ROW>
				
				</FIELDS>
				<FOOTER>
				
						<ROW>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" COLSPAN="2" CELLCLASS="TOTALES">"TOTAL PRODUCTOS"</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("precio_suma")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("importe_suma")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
						</ROW>
						<ROW>
							<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
						</ROW>
				
				<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="13" HEIGHT="20" WIDTH="650"></COL>
				</ROW>
				</FOOTER>
			</GROUP>
			
		</GROUP>
		
	</GROUPS>
</REPORT>