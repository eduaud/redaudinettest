<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<REPORT MARGINWIDTH="1" MARGINHEIGHT="1" TITLE="inventario">
	<TITLE>Pedidos Detallado</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>	
	<CSS MEDIA="screen">../../css/reportesPantalla.css</CSS>
	<CSS MEDIA="print">../../css/reportesPantalla.css</CSS>
	
	<NO_DATA_MSG>No se encontraron datos que cumplan los criterios, intente nuevamente. Verifique el estado del documento antes de mandar a impresi√≥n.</NO_DATA_MSG>

	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="CELDAGRANTOTAL" TEXTCLASS="BOLD" COLSPAN="21" WIDTH="750">"NUMERO TOTAL DE REGISTROS: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>

	<PAGE BORDER="0" SIZE="6666666666" CELLSPACING="1" CELLPADDING="1" ALIGN="CENTER">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="21" WIDTH="750">
					<XHTML>
						<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="1" WIDTH="100%">
							<TR>
								<TD ROWSPAN="2" CLASS="HEADER" style="background:#FFFFFF; font-size:10px" ALIGN="CENTER" WIDTH="40%">
									<IMG SRC="../../imagenes/sitio_base/tip_logo.png"/><BR/>
									<b>NASSER</b>
								</TD>
								<TD CLASS="HEADER" WIDTH="60%" ALIGN="CENTER">									
									<b><U>Pedidos Detallado</U></b>																		
								</TD>
							</TR>
						</TABLE>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("filtro")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" COLSPAN="21" WIDTH="750" TYPE="EXPRESSION" CELLCLASS="HEADER">" ".$this->getParameter("rango_fechas")</COL>
			</ROW>
			<ROW>
				<COL ALIGN="RIGHT" COLSPAN="21" WIDTH="700" TYPE="EXPRESSION" CELLCLASS="HEADER">insert_date()</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
			</ROW>
			<ROW>
				<COL ALIGN="LEFT" HEIGHT="15" COLSPAN="21" WIDTH="750">
					<XHTML>
						<BUTTON style='font-family:Verdana, Arial, Helvetica, sans-serif; font-size:10px; font-weight:bolder; background-color:#FFFFFF; color:#094932; border:1px solid #094932;' onClick="window.print();" class="botonImprimir">IMPRIMIR</BUTTON>
					</XHTML>
				</COL>
			</ROW>
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>					
			<ROW>
				<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750">""</COL>
			</ROW>
		</HEADER>
		
		<FOOTER>			
			<ROW>
				<COL ALIGN="CENTER" COLSPAN="21" TYPE="EXPRESSION" CELLCLASS="NUMPAG">"Pagina ".$this->getPageNum()</COL>
			</ROW>		
		</FOOTER>
	</PAGE>

	<GROUPS>
		<GROUP NAME="control_pedido" EXPRESSION="control_pedido">
			<HEADER>
					<ROW>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="70" >"SUCURSAL"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="40" >"PEDIDO"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"CLIENTE"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="150" >"VENDEDOR"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="40" >"FECHA"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"IMPORTE"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"FLETES"</COL>
							<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="SUBHEADEREVEN" TEXTCLASS="BOLD" WIDTH="50" >"PRODUCTOS"</COL>
					</ROW>
					
					<ROW>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="70">$this->getValue("sucursal")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="40">$this->getValue("pedido")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="150">$this->getValue("cliente")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="150">$this->getValue("vendedor")</COL>
							<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="40">$this->getValue("fec_alta")</COL>
							<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="50">$this->getValue("importe")</COL>
							<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="50">$this->getValue("fletes")</COL>
							<COL TYPE="EXPRESSION" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="50">$this->getValue("productos")</COL>
					</ROW>
			</HEADER>
					
			<GROUP NAME="detalle">
				<HEADER>
						<ROW>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="100" >"SKU"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="150" >"PRODUCTO"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="50" >"CANTIDAD"</COL>
								<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="TITULODETALLE" TEXTCLASS="BOLD" WIDTH="70" >"IMPORTE"</COL>
						</ROW>
				</HEADER>
				<FIELDS>
					<ROW>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="100">sku</COL>
							<COL TYPE="FIELD" ALIGN="LEFT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="150">producto</COL>
							<COL TYPE="FIELD" ALIGN="CENTER" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="50">cantidad</COL>
							<COL TYPE="FIELD" ALIGN="RIGHT" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" COLSPAN="1" WIDTH="70">importe_prod</COL>
					</ROW>
				
				</FIELDS>
				<FOOTER>
				
						<ROW>
							<COL ALIGN="RIGHT" TYPE="EXPRESSION" COLSPAN="3" CELLCLASS="TOTALES">"TOTAL PRODUCTOS"</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES">$this->getSum("importe_prod_suma")</COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
							<COL ALIGN="RIGHT" NUMBERFORMATEX="2" TYPE="EXPRESSION" CELLCLASS="MONTOSTOTALES"></COL>
						</ROW>
						<ROW>
							<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="21" HEIGHT="10" WIDTH="750"></COL>
						</ROW>
				
				<ROW>
					<COL ALIGN="CENTER" CELLCLASS="ESPACIO" TYPE="EXPRESSION" COLSPAN="13" HEIGHT="20" WIDTH="650"></COL>
				</ROW>
				</FOOTER>
			</GROUP>
			
		</GROUP>
		
	</GROUPS>
</REPORT>